  "author": "dacc@ukhomeoffice",
  "license": "MIT",
  "dependencies": {
    "async": "^2.6.3",
    "aws-sdk": "^2.736.0",
    "bluebird": "^3.7.2",
    "merge": ">=1.2.1",
    "moment": "^2.27.0",
    "mongodb": "^3.6.0",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-jest": "^26.3.0",
    "husky": "^4.2.5",
    "jest": "^26.4.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^1.11.1"
  },
  "engines": {
    "node": ">=10.6.0"
  },
  "jest": {
